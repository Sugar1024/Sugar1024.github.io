  <entry>
    <title>人工智能对安全的影响</title>
    <link href="http://example.com/2022/02/07/%E4%BA%BA%E5%B7%A5%E6%99%BA%E8%83%BD%E5%AF%B9%E5%AE%89%E5%85%A8%E7%9A%84%E5%BD%B1%E5%93%8D0/"/>
